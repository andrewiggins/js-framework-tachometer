{
  "name": "js-framework-tachometer",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "A comparison of the perfomance of a few popular javascript frameworks using tachometer",
  "scripts": {
    "setup": "node ./scripts setup",
    "build": "node ./scripts build",
    "bench": "node ./scripts bench",
    "publish": "cross-env NODE_DEBUG=gh-pages node ./scripts publish",
    "start": "sirv --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewiggins/js-framework-tachometer.git"
  },
  "author": "Andre Wiggins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewiggins/js-framework-tachometer/issues"
  },
  "homepage": "https://github.com/andrewiggins/js-framework-tachometer#readme",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,yml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "cpy": "^8.1.0",
    "cross-env": "^7.0.2",
    "del": "^6.0.0",
    "dot": "^1.1.3",
    "globby": "^11.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mem": "^8.0.0",
    "normalize-path": "^3.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.40.0",
    "sade": "^1.7.4",
    "sirv-cli": "^1.0.11",
    "tachometer": "^0.7.0"
  },
  "dependencies": {
    "afterframe": "^1.0.1",
    "spectre.css": "^0.5.9"
  }
}
